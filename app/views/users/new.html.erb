<h1>Novo Usuário</h1>

<%= form_with(model: @user, local: true) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.submit 'Criar Usuário' %>
  </div>
<% end %>

<%= link_to 'Voltar', users_path %>
